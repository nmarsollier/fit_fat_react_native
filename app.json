{
  "expo": {
    "name": "FitFat",
    "displayName": "FitFat",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nmarsollier.fitfat"
    },
    "scheme": "com.nmarsollier.fitfat",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-router",
        {
          "root": "./app/routes"
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.nmarsollier.fitfat"
    }
  }
}